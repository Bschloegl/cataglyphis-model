<?xml version="1.0"?>
<launch>

  <!-- Convention for comments in this .launch-file
       If comments begins with ... than
       Launch ...                  = another .launch-file is included equivalent to [roslaun pkg .launch-file]
       Run ...                     = a node is run equivalent to [rosrun pkg node]
       Load ...                    = a parameter is loaded from parameter server
  -->
  
  <!-- Launch joint controllers -->
 <!-- <include file="$(find antleg_control)/launch/antleg_control.launch" /> -->

  <!-- Run node to "fork" output verctor q [1xN] of inverse kinematic to single joints -->
  <node name="antleg_ikin2sjoint" pkg="antleg_matlab" type="antleg_matlab" >
    <remap from="/robot_joint1/command" to="/phantomx/j_c1_lf_position_controller/command/" /> 
    <remap from="/robot_joint2/command" to="/phantomx/j_c2_lf_position_controller/command/" />
    <remap from="/robot_joint3/command" to="/phantomx/j_thigh_lf_position_controller/command/" />
    <remap from="/robot_joint4/command" to="/phantomx/j_tibia_lf_position_controller/command/" />
  </node>

</launch>
